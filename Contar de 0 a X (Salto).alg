algoritmo "conte ate 10"
var
contador,salto ,valor: inteiro
inicio
Escreva("Escreva um número para contarmos desde o 0: ")
leia(valor)
Escreva("Qual o valor do salto? ")
leia(salto)
contador <- 0
Se (valor>1000)entao
   escreva("Não temos tempo pra isso")
   senao
enquanto (contador<=valor)  faca
   Escreval(contador)
   contador <- contador+salto
fimenquanto
fimse
fimalgoritmo
