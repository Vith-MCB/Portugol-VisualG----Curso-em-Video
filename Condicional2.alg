algoritmo "dinheiro para Disney"

var
d1:real
inicio
Escreva("Quanto dineiro nos temos? ")
leia(d1)
Escreval("Então temos ",d1,"?!")
Se(d1>=5000)entao
   escreva("Partiu Disney")
senao
   se(d1>=3500)e(d1<5000)entao
      escreva("Parece que vamos para o Beto Carrero")
   senao
      Escreva("Vai trabalhar vagabundo")
   fimse
fimse

fimalgoritmo
